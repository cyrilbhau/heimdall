DATABASE_URL="postgresql://user:password@host:5432/dbname?schema=public"
ADMIN_PASSWORD="set-a-strong-admin-password"
ADMIN_SESSION_SECRET="set-a-long-random-session-secret"
CRM_PROVIDER="NONE"

# Railway Buckets (use Variable Reference from your Bucket service, or set manually)
# For S3 API you must use BUCKET (globally unique name), not the display name.
BUCKET="your-bucket-unique-name"
ENDPOINT="https://storage.railway.app"
REGION="auto"
ACCESS_KEY_ID="from-railway-bucket-credentials"
SECRET_ACCESS_KEY="from-railway-bucket-credentials"
# Alternative env names also supported:
# AWS_ACCESS_KEY_ID / AWS_SECRET_ACCESS_KEY, RAILWAY_BUCKET_NAME, RAILWAY_BUCKET_ENDPOINT


